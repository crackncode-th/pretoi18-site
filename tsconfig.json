{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "paths": {
            "$data": ["src/data"],
            "$data/*": ["src/data/*"],
            "$icons": ["src/icons"],
            "$icons/*": ["src/icons/*"],
            "$lib": ["src/lib"],
            "$lib/*": ["src/lib/*"]
        },
        "resolveJsonModule": true
    }
}
